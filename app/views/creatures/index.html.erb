<h1>Creatures#index</h1>
<p>Find me in app/views/creatures/index.html.erb</p>


<%= link_to "home", root_path  %>  <!-- first argument is words, second is path -->

<hr>

<% @creatures.each do |creature| %>
	<div>
		<%= link_to "Read more", creature_path(creature) %> 
		<p><%= creature.name %></p>
		<span> <%= creature.description %> </span>
		<hr>
	</div>
<% end %>
